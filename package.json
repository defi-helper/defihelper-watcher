{
  "name": "dfh-watcher",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "lint-staged",
    "server:build": "tsc -p ./server",
    "server:watch": "tsc-watch -p ./server --pretty --onSuccess 'node ./server/dist/index.js'",
    "client:build": "parcel build ./client/index.tsx --dist-dir ./public/dist",
    "client:watch": "parcel watch ./client/index.tsx --dist-dir ./public/dist",
    "start": "node ./server/dist",
    "events:resolve": "node ./server/dist/eventsResolve.js",
    "events:log": "node ./server/dist/eventsLog.js",
    "queue:consume": "node ./server/dist/queueConsume.js",
    "queue:deferred": "node ./server/dist/queueDeferred.js",
    "queue:broker": "node ./server/dist/queueBroker.js",
    "queue:schedule": "node ./server/dist/queueSchedule.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./server/src/**/*.ts": [
      "eslint --ignore-path .gitignore --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Bond Appetit",
  "license": "MIT",
  "dependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/bluebird": "^3.5.33",
    "@types/command-line-args": "^5.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/redis": "^2.8.32",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "body-parser": "^1.19.2",
    "command-line-args": "^5.2.0",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-typescript": "^0.14.0",
    "ethers": "5.3.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "knex": "^0.21.0",
    "lint-staged": "^10.5.4",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "parcel": "^2.6.0",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "pretty-js": "^0.2.1",
    "rabbit-queue": "^5.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redis": "^3.1.2",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.3",
    "use-debounce": "^6.0.1",
    "uuid": "^8.3.2",
    "ws": "^8.8.0"
  },
  "_moduleAliases": {
    "@container": "server/dist/container",
    "@services": "server/dist/services",
    "@models": "server/dist/models",
    "@api": "server/dist/api"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "process": "^0.11.10",
    "husky": "^8.0.0"
  }
}
